{
  "pipelines": {
    "pipeline_1": {
      "preprocessing": {
        "tokenize": {
          "function": "tokenize",
          "async": true,
          "params": {
            "param1": "value1"
          }
        },
        "normalize": {
          "function": "normalize",
          "async": false,
          "params": {
            "param1": "value1"
          }
        },
        "remove_stopwords": {
          "function": "remove_stopwords",
          "async": true,
          "params": {
            "param1": "value1"
          }
        }
      },
      "analysis": [
        {
          "task": "keyword_extraction",
          "params": {
            "method": "tfidf",
            "top_n": 10
          },
          "metrics": {
            "precision_threshold": 0.80
          },
          "async": true
        }
      ],
      "postprocessing": {
        "export_results": {
          "function": "export_results",
          "params": {
            "path": "./pipeline1_results.csv"
          },
          "async": false
        }
      }
    }
  }
}
